  <title>
    Vakanz Results Page <!-- SEO title -->
  </title>
</head>

<body>
  <!-- search bar -->
  <!-- fix sticky header -->

<header>
  <div id="sticker_results">
    <div class="search">
      <div class="container">
        <div class="row">
          <div class="col-md-10 col-md-offset-1">
            <div class="form-section">
              <div class="row">
                <form role="form">
                  <div class="col-md-3 col-sm-3 col-xs-8 no-padding">
                    <div class="form-group">
                      <div class="ui-widget">
                        <label class="sr-only" for="location">Location</label>
                        <input class="form-control" id="location" 
                          placeholder= "Where would you like to go?" 
                          type="text"
                          <%= params[:location] && !params[:location].empty? ?
                              "value='#{params[:location]}'" : "" %>
                        >
                      </div>
                    </div>
                  </div>

                  <div class="col-md-3 col-sm-3 col-xs-3 no-padding">
                    <div class="form-group">
                      <label class="sr-only" for="checkin">Start date</label>
                      <div class="input-group">
                        <input class="form-control" id="datepicker" placeholder=
                        "Start date" type="text"> <span class=
                        "input-group-addon"><i class=
                        "glyphicon glyphicon-calendar"></i></span>
                      </div>
                    </div>
                  </div>
                  <div class="col-md-3 col-sm-3 col-xs-3 no-padding">
                    <div class="form-group">
                      <label class="sr-only" for="checkout">End date</label>
                      <div class="input-group">
                        <input class="form-control" id="checkout" placeholder=
                        "End date" type="text"> <span class=
                        "input-group-addon"><i class=
                        "glyphicon glyphicon-calendar"></i></span>
                      </div>
                    </div>
                  </div>
                  <div class="col-md-3 col-sm-3 col-xs-1">
                    <button class="btn btn-default btn-primary" id="submit"
                    type="submit">Search</button>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>

   <br>


  <div class="container">
    <div class="top_space">
      <h2><span id="result_search">San Francisco</span></h2>
    </div>
  </div>

  <div class="container">
    <div class="row">
      <div class="col-md-4 no-padding">
        <a href="#"><img src=" http://placehold.it/140x100" alt="weather_info"></a>    
      </div>
      <div class="col-md-8 col-sm-6 col-xs-12 no-padding">
        <div id="wn2"></div> <!-- google map -->
      </div>
    </div>
  </div>
